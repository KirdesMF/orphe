// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  password  String
  createdAt DateTime
  updatedAt DateTime
}

model Song {
  id        Int      @id @default(autoincrement())
  title     String
  artist    String
  year      Int
  createdAt DateTime
  updatedAt DateTime
}
